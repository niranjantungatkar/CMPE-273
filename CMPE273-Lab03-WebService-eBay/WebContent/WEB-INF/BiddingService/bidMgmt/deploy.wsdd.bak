<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from BiddingService WSDL service -->

  <service name="Bidding" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://bidMgmt"/>
      <parameter name="wsdlServiceElement" value="BiddingService"/>
      <parameter name="schemaQualified" value="http://bidMgmt"/>
      <parameter name="wsdlServicePort" value="Bidding"/>
      <parameter name="className" value="bidMgmt.BiddingSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Bidding"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="updateBidLog" qname="operNS:updateBidLog" xmlns:operNS="http://bidMgmt" soapAction="" >
        <parameter qname="pns:product_id" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:bidamount" xmlns:pns="http://bidMgmt" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:bidder" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:time" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="updateProductTable" qname="operNS:updateProductTable" xmlns:operNS="http://bidMgmt" soapAction="" >
        <parameter qname="pns:product_id" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:bidder" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:bidamount" xmlns:pns="http://bidMgmt" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="update_bid_transaction" qname="operNS:update_bid_transaction" xmlns:operNS="http://bidMgmt" soapAction="" >
        <parameter qname="pns:product_id" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:username" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="update_transaction_detail" qname="operNS:update_transaction_detail" xmlns:operNS="http://bidMgmt" soapAction="" >
        <parameter qname="pns:username" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:product_id" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:bid_amount" xmlns:pns="http://bidMgmt" type="tns:float" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:time" xmlns:pns="http://bidMgmt" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="updateBidLog updateProductTable update_bid_transaction update_transaction_detail"/>

  </service>
</deployment>
